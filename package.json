{
  "name": "web-frontend-unit-tests",
  "version": "0.0.0",
  "description": "Web Frontend Unit Tests",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node .",
    "test": "echo 'run test-ng or test-react'",
    "test-ng": "NODE_ENV=test node_modules/karma/bin/karma start ng/karma.conf.js",
    "test-react": "NODE_ENV=test node_modules/jest-cli/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobiquity/WebFrontEndUnitTests.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mobiquity/WebFrontEndUnitTests/issues"
  },
  "homepage": "https://github.com/Mobiquity/WebFrontEndUnitTests#readme",
  "dependencies": {
    "angular": "^1.5.0",
    "babel-core": "^6.6.0",
    "babel-loader": "^6.2.4",
    "babel-preset-angular": "^6.0.15",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "html-loader": "^0.4.3",
    "ng-annotate-loader": "^0.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "chai": "^3.5.0",
    "connect": "^3.4.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "jest": "^0.1.40",
    "jest-cli": "^0.9.0",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^0.5.4",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "serve-static": "^1.10.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/(?!(fbjs/lib/ErrorUtils.js$|fbjs/lib/fetch.js$|fbjs/lib/fetchWithRetries.js$))"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "spec.js",
      "spec.jsx"
    ],
    "testDirectoryName": "react"
  }
}
